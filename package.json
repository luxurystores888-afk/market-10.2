  "scripts": {
    "dev": "vite",
    "dev:server": "cross-env NODE_ENV=development PORT=3001 tsx api/index.ts",
    "build": "vite build && esbuild api/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && node scripts/add-sri.js",
    "start": "NODE_ENV=production node dist/index.js",
    "db:push": "drizzle-kit push",
    "quick-start": "npm install && npm run db:push && npm run dev:all",
    "automation:start": "curl -X POST http://localhost:3001/api/automation/start",
    "automation:stop": "curl -X POST http://localhost:3001/api/automation/stop",
    "automation:status": "curl http://localhost:3001/api/automation/status",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "complete-automation": "node scripts/master-automation.js",
    "production": "pm2 start ecosystem.config.js",
    "production:stop": "pm2 stop all",
    "production:restart": "pm2 restart all",
    "production:logs": "pm2 logs",
    "production:status": "pm2 status",
    "blueprint": "node scripts/generate-blueprint.js",
    "blueprint:watch": "nodemon --watch src --watch api --exec npm run blueprint",
    "security-test": "node scripts/security-test.js",
    "bug-detection": "node scripts/bug-detection.js",
    "activate-maximum-profit": "curl -X POST http://localhost:3001/api/activate-maximum-profit",
    "lint": "eslint . --ext .ts,.tsx"
  },
