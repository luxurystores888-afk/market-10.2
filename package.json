{
  "name": "cyber-mart-2077",
  "version": "1.0.0",
  "description": "CYBER MART 2077 - ULTIMATE CYBERPUNK E-COMMERCE",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:server": "cross-env NODE_ENV=development PORT=3001 tsx api/index.ts",
    "build": "vite build && esbuild api/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "db:push": "drizzle-kit push",
    "quick-start": "npm install && npm run db:push && npm run dev:all",
    "automation:start": "curl -X POST http://localhost:3001/api/automation/start",
    "automation:stop": "curl -X POST http://localhost:3001/api/automation/stop",
    "automation:status": "curl http://localhost:3001/api/automation/status",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
